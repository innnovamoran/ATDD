  "scripts": {
    "esbuild:prod": "./esbuild.js",
    "start": "npm run esbuild:prod && nodemon bundle/index",
    "dev": "nodemon src/index.ts",
    "test": "jest --forceExit --coverage --verbose",
    "migration:create": "npx sequelize migration:generate --migrations-path ./src/Core/Migrations --name ",
    "seeder:create": "npx sequelize seed:generate --seeders-path ./src/Core/Seeder --name ",
    "migration:run": "npx sequelize db:migrate --config ./src/Server/Config/Sequelize/config.js --migrations-path ./src/Core/Migrations",
    "migration:dev:run": "npx sequelize db:migrate --config ./Config/config.js --migrations-path ./Migrations",
    "seeder:run": "npx sequelize db:seed:all --config ./src/Server/Config/Sequelize/config.js --seeders-path ./src/Core/Seeder",
    "seeder:dev:run": "npx sequelize db:seed:all --config ./Config/config.js --seeders-path ./Seeder"
  },
  "dependencies": {
    "@babel/preset-env": "^7.19.0",
